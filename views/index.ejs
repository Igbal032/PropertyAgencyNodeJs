    <div class="row">
        <% for(let n of allNews2){ %>
        <div class="col-lg-4 col-md-6 mb-4">
          <div class="latest-post">
              <div class="latest-post-media">
                <a href="/news/<%=n.id%>" class="latest-post-img">
                    <img style="max-height: 245px;min-height: 245px;width: 100%;object-fit: cover;" loading="lazy" class="img-fluid" src="<%= (n.imgPath.length < 6) ? 'https://upload.wikimedia.org/wikipedia/commons/thumb/6/69/How_to_use_icon.svg/2214px-How_to_use_icon.svg.png' : ('/'+n.imgPath)%>" alt="img">
                </a>
              </div>
              <div class="post-body">
                <h4 class="post-title">
                    <a href="/news/<%=n.id%>" class="d-inline-block"><%=n.title%></a>
                </h4>
                <div class="latest-post-meta">
                    <span class="post-item-date">
                      <i class="fa fa-clock-o"></i> June 17, 2017
                    </span>
                </div>
              </div>
          </div><!-- Latest post end -->
        </div><!-- 2nd post col end -->

